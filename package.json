{
  "name": "expense-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/error-message": "0.0.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "chokidar-cli": "^2.1.0",
    "dayjs": "^1.8.29",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-alert": "^7.0.2",
    "react-calendar": "^3.1.0",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.0.4",
    "react-loading-skeleton": "^2.1.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-tooltip": "^4.2.7",
    "sass": "^1.26.10",
    "swr": "^0.2.3",
    "tailwindcss": "^1.5.2"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "start:react": "react-scripts start",
    "prebuild": "run-s build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001"
}
